<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alexplanasobany7.utemp">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.alexplanasobany7.utemp.ConfiguracionActivity"></activity>
        <activity android:name="com.alexplanasobany7.utemp.Registro_Activity" />
        <activity android:name="com.alexplanasobany7.utemp.Main_Menu" />
        <activity android:name="com.alexplanasobany7.utemp.Marcas" />
        <activity android:name="com.alexplanasobany7.utemp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>